{
    "name": "exponentiles/engine",
    "description": "The game logic.",
    "keywords": [
        "exponentiles",
        "engine"
    ],
    "homepage": "https://github.com/exponentiles/engine",
    "license": "MIT",
    "authors": [
        {
            "name": "jstoone",
            "email": "jstoone@drk.sh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.34"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "nunomaduro/collision": "^5.3",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Exponentiles\\Engine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Exponentiles\\Engine\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
